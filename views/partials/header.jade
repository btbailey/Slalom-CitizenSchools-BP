#header-wrapper
  a.show-for-medium-down.left-off-canvas-toggle(href="#")
    i(style="padding-left: 5px;").fa.fa-bars.fa-2x
    nav#nav-small.left-off-canvas-menu(role="navigation")
      ul.off-canvas-list
        li.active
          a(href="/") Home!
        li
          a(href="/about-us") About Us
        li
          a(href="#feedback") Feedback
  .row.show-for-large-up
    .small-12.medium-12.large-12.columns
      .contain-to-grid
        nav#nav-section.top-bar(role="navigation", data-topbar)
          section.top-bar-section
            ul
              li.medium-4.large-4.active
                a(href="/") HomeXX
              li.medium-4.large-4
                a(href="/about-us") About Us
              li.medium-4.large-4
                a(href="#feedback") Feedback